<html>
    <body>
        <div><a href="/">Go to Home page</a></div>
        <h2>U can view, edit and delete the record</h2>
        <div style="width:1000px; margin: 0 auto;">
        <table>
            <tr>
                <th>S.No</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Date of Birth</th>
                <th>Action</th>
            </tr>
            <% 
            for(var i=0;i< userlistobj.length;i++)
            {
                %>
                <tr>
                    <td> <%= i+1%></td>
                    <td><%= userlistobj[i].fname %></td>
                    <td><%= userlistobj[i].lname %></td>
                    <td><%= userlistobj[i].email %></td>
                    <td><% let dob= new Date(userlistobj[i].dob);
                    let userformateDate= dob.getDay()+"-"+dob.getMonth()+"-"+dob.getYear;
                    %>
                    <%= userformateDate%>
                    </td>
                    <td><a href="edituser?uid=<%=userlistobj[i].uid %>">Edit</a></td>
                </tr>
        <%    } %>
        </table>
        </div>
    </body>
</html>