<html>
    <body>
        <form action="/editusersubmit" method="post">
            <table>
                <tr>
                    <td>First name</td>
                    <td><input type="text" name="fname" value="<%=data[0].fname%>"/></td>
                </tr>
                <tr>
                    <td>Last Name</td>
                    <td><input type="text" name="lname" value="<%=data[0].lname%>"/></td>
                </tr>
                <tr>
                    <td>Email</td>
                    <td><input type="email" name="email" value="<%=data[0].email%>"/></td>
                </tr>
                <input type="hidden" name="uid" value="<%=data[0].uid%>"/>
                <tr>
                    <td colspan="2" align="center"><input type="submit" value="Update user"/></td>
                </tr>
            </table>
        </form>
    </body>
</html>